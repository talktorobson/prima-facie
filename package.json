{
  "name": "prima-facie",
  "version": "11.0.0",
  "description": "Legal-as-a-Service Platform - Sistema Completo de Gestão para Escritórios de Advocacia Brasileiros",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ops": "node tests/ops/ops-test-runner.js",
    "test:ops:verbose": "node --expose-gc tests/ops/ops-test-runner.js",
    "test:database": "node tests/database/run-tests.js",
    "test:database:coverage": "node tests/database/run-tests.js --coverage",
    "test:database:verbose": "node tests/database/run-tests.js --verbose",
    "test:database:watch": "node tests/database/run-tests.js --watch",
    "test:database:bail": "node tests/database/run-tests.js --bail",
    "test:database:schema": "node tests/database/run-tests.js --suite=schema",
    "test:database:integrity": "node tests/database/run-tests.js --suite=integrity",
    "test:database:functions": "node tests/database/run-tests.js --suite=functions",
    "test:database:triggers": "node tests/database/run-tests.js --suite=triggers",
    "test:database:performance": "node tests/database/run-tests.js --suite=performance",
    "test:all": "npm run test && npm run test:database",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "legal",
    "law",
    "saas",
    "management"
  ],
  "author": "Robson Benevenuto D'Avila Reis",
  "license": "ISC",
